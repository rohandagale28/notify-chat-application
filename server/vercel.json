{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://notify-chat-application-git-development-rohandagale28s-projects.vercel.app",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    }
  ]
}
